@{
    ViewData["Title"] = "Vælg aktivt spørgeskema";
}
<h1 class="text-center">Vælg aktivt spørgeskema</h1>
<div class="col-md-8 col-md-offset-2">
    <h2>Gemte spørgeskemaer:</h2>
    <div class="table-responsive ">
        <table class="table">
            <thead>
                <tr>
                    <th>Navn</th>
                    <th>Oprettelsesdato</th>
                    <th>Antal besvarelser</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>Udskrivning</td>
                    <td>20-02-2017</td>
                    <td>15</td>
                    <td><input type="radio" name="optradio"></td>
                </tr>
                <tr>
                    <td>Indskrivning</td>
                    <td>05-02-2017</td>
                    <td>30</td>
                    <td><input type="radio" name="optradio"></td>
                </tr>
                <tr>
                    <td>Undersøgelse</td>
                    <td>20-03-2017</td>
                    <td>70</td>
                    <td><input type="radio" name="optradio"></td>
                </tr>
            </tbody>
        </table>
    </div>
    <div class="text-center col-xs-12">
        <button type="submit" class="btn btn-primary btn-lg">Vælg som aktiv</button>
    </div>
</div>


