                if (inputname.value.length > 0) {
                    var exists = false;
                    for(var i = 0; i<names.length; i++){
                        if(inputname.value == names[i])
                            exists = true;
                    }
                    if(!exists){

                        var nameempty = true;

                        for(var i = 0; i<@q_amt; i ++){
                            var qname1 = document.getElementById("q" + i + "nameid");
                            if(qname1.value.length >0){
                                nameempty = false;
                            }
                            
                        
                        }

                        if(!nameempty){
                            $('#questioncontainer').submit();
                        }
                        else{
                            alert('Du har ikke indtastet nogle spørgsmål.')
                        
                        }
                    
                    }
                    else{
                        alert('Spørgeskema eksisterer allerede med dette navn. Indtast et andet navn.')
                    
