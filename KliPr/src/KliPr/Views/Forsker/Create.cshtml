    var count = 0;

    document.getElementById("plusbtn").onclick = function () { plusbtn() };

    function plusbtn() {

        var myOpts = document.getElementById('sel').options;
        if(count < myOpts.length-1){
            myOpts[count + 1].style.display = 'block'

            count++;

            for (var i = 0; i < @q_amt; i++)
            {      
                $("#q" + i).hide();
                
            }

            $("div.floatleft select").val(count);
            $("#q" + count).show();

        }
    }

    function minusbtn() {

        var myOpts = document.getElementById('sel').options;
        if(count>0){
        myOpts[count].style.display = 'none'
        count--;

        var sel = document.getElementById("sel");
        var selval = sel.options[sel.selectedIndex].value;


        if (selval > count) {
            $("div.floatleft select").val(count);


            for (var i = 0; i <= @q_amt -1; i++)
                $("#q" + i).hide();

            $("#q" + count).show();
            }
        }

